{
  "name": "vue2-ts-template",
  "version": "0.1.0",
  "license": "ISC",
  "scripts": {
    "serve": "npm run serve:dev",
    "serve:dev": "vue-cli-service serve --open --mode serve-dev",
    "serve:prod": "vue-cli-service serve --open --mode serve-prod",
    "serve:release": "vue-cli-service serve --open --mode serve-release",
    "serve:mock": "cross-env use_mock=true npm run serve",
    "build": "npm run build:dev",
    "build:dev": "vue-cli-service build --mode build-dev",
    "build:release": "vue-cli-service build --mode build-release",
    "build:prod": "vue-cli-service build --mode build-prod",
    "build:analyzer": "cross-env use_analyzer=true npm run build",
    "lint": "vue-cli-service lint",
    "lint:all": "npm run lint & npm run lint:prettier & npm run lint:stylelint",
    "lint:prettier": "prettier --write --list-different --loglevel warn \"**/*.{js,ts,jsx,tsx,json,css,less,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"src/**/*.{vue,less}\"",
    "new:component": "hygen component new",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.13.5",
    "core-js": "^2.6.5",
    "element-ui": "^2.15.10",
    "localforage": "^1.7.3",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.6.5",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2",
    "vuex-map-fields": "^1.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@vue/cli-plugin-babel": "^3.1.0",
    "@vue/cli-plugin-eslint": "^3.1.0",
    "@vue/cli-plugin-typescript": "^3.1.0",
    "@vue/cli-service": "^3.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "chalk": "^5.1.0",
    "commander": "^9.4.1",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "hygen": "^6.2.11",
    "inquirer": "^9.1.3",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.17",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.13.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard": "^28.0.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^4.6.1"
  }
}
